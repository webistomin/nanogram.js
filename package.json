{
  "name": "nanogram.js",
  "version": "1.0.2",
  "description": "An easy-to-use and simple Instagram package that allow you to fetch media content without API and access token.",
  "reveal": true,
  "keywords": [
    "instagram",
    "instagram-scrapping",
    "instagram-photos",
    "instafeed",
    "instagram-api"
  ],
  "main": "./dist/nanogram.cjs.js",
  "module": "./dist/nanogram.esm.js",
  "browser": {
    "dist/nanogram.js": "./dist/nanogram.iife.js",
    "dist/nanogram.esm.js": "./dist/nanogram.esm.js"
  },
  "unpkg": "./dist/nanogram.iife.js",
  "jsdelivr": "./dist/nanogram.iife.js",
  "typings": "./dist/nanogram.d.ts",
  "files": [
    "dist/*",
    "src/*"
  ],
  "scripts": {
    "build": "run-s prebuild build:all build:dts other:typedoc",
    "build:all": "cross-env NODE_ENV=production rollup --config @config/rollup.config.js",
    "build:cjs": "cross-env NODE_ENV=production rollup --config @config/rollup.config.js --format cjs",
    "build:dts": "tsc -d --declarationDir dist --declarationMap --emitDeclarationOnly",
    "build:es": "cross-env NODE_ENV=production rollup --config @config/rollup.config.js --format es",
    "build:iife": "cross-env NODE_ENV=production rollup --config @config/rollup.config.js --format iife",
    "dev": "rollup -c @config/rollup.config.js -w",
    "lint": "run-s other:eslint:write other:prettier:write",
    "other:eslint:write": "eslint --ext .js,.ts {src,test}/* --fix --max-warnings 0",
    "other:prettier:write": "prettier --write \"{src,test}/**/*.{js,ts}\"",
    "other:sizelimit": "size-limit",
    "other:typedoc": "typedoc --out docs --target es6 --theme minimal --mode file src",
    "prebuild": "rimraf dist",
    "start": "npm run dev",
    "test": "jest --config jest.config.js --coverage",
    "test:watch": "jest --config jest.config.js --coverage --watch"
  },
  "repository": {
    "type": "git",
    "url": "githttps://github.com/webistomin/nanogram.js.git"
  },
  "author": "webistomin",
  "email": "webistomin@gmail.com",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "bugs": {
    "url": "https://github.com/webistomin/nanogram.js/issues"
  },
  "homepage": "https://github.com/webistomin/nanogram.js#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@ls-lint/ls-lint": "^1.8.0",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-inject": "^4.0.1",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@size-limit/preset-small-lib": "^4.4.5",
    "@size-limit/time": "^4.4.5",
    "@types/jest": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^26.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^10.1.3",
    "lodash.capitalize": "^4.2.1",
    "minimist": "^1.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.6.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-banner2": "^1.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "scriptlint": "^2.1.1",
    "size-limit": "^4.4.5",
    "ts-jest": "^25.3.1",
    "tslib": "^1.11.1",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3"
  },
  "dependencies": {}
}
